# Google Cloud Configuration
# Replace with your actual Project ID, Service Account Key path and region
GOOGLE_PROJECT_ID=your-project-id-here
GOOGLE_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=C:/path/to/your/google_key.json
GOOGLE_SUMMARY_KEY=your-gemini-api-key
SUMMARY_MODEL=models/gemini-2.5-flash
SUMMARY_FALLBACK_MODEL=models/gemini-2.5-flash
SUMMARY_MAX_OUTPUT_TOKENS=900
SUMMARY_CHUNK_CHARS=12000
TITLE_MODEL=models/gemini-2.5-flash
TITLE_FALLBACK_MODEL=models/gemini-2.5-flash
TITLE_TEMPERATURE=0.4
TITLE_MAX_OUTPUT_TOKENS=24

# Audio Settings
AUDIO_SAMPLE_RATE=16000
TRANSCRIPTION_LANGUAGES=en-US,nl-NL,de-DE
TRANSCRIPTION_CHUNK_SECONDS=55
