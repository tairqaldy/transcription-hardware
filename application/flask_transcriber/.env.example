# Supabase (same values as VITE_SUPABASE_URL / VITE_SUPABASE_ANON_KEY)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_BUCKET=audio

# Google Cloud Speech-to-Text credentials
GOOGLE_APPLICATION_CREDENTIALS=C:\\path\\to\\google-key.json

# Gemini summarization
GOOGLE_SUMMARY_KEY=
GEMINI_MODEL=gemini-2.5-flash

# Optional
TRANSCRIBE_LANGUAGE=en-US
FLASK_SECRET_KEY=change-me
SPEECH_FORCE_LONG_RUNNING=true
SPEECH_LONG_RUNNING_TIMEOUT=600
